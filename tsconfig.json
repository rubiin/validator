{
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "module": "esnext",
        "alwaysStrict": false,
        "strict": true,
        "noImplicitAny": false,
        "resolveJsonModule": true,
        "plugins": [
            {
                "name": "typescript-deno-plugin"
            }
        ],
        "target": "esnext"
    }
}
